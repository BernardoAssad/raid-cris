{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/events",
        "dest": "server.js",
        "headers": {
          "Cache-Control": "no-cache, no-store, must-revalidate",
          "Connection": "keep-alive"
        }
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }